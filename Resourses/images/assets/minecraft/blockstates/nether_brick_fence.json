{
    "multipart": [
        {   "apply": [
        	{ "model": "fence/nether_post" },
        	{ "model": "fence/nether_post", "y": 90, "uvlock": false },
        	{ "model": "fence/nether_post", "y": 180, "uvlock": false },
        	{ "model": "fence/nether_post", "y": 270, "uvlock": false }
        	]
        },
        
        {   "when": { "north": "false" },
            "apply": { "model": "fence/nether_post_ns", "uvlock": false }
        },
        {   "when": { "south": "false" },
            "apply": { "model": "fence/nether_post_ns", "y": 180, "uvlock": false }
        },
        {   "when": { "west": "false" },
            "apply": { "model": "fence/nether_post_ew", "uvlock": false }
        },
        {   "when": { "east": "false" },
            "apply": { "model": "fence/nether_post_ew", "y": 180, "uvlock": false }
        },    
        
        
        {   "when": { "north": "true" },
            "apply": { "model": "fence/nether_slats_ns", "uvlock": false }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "fence/nether_slats_ew", "y": 180, "uvlock": false }
        },
        {   "when": { "south": "true" },
            "apply": { "model": "fence/nether_slats_ns", "y": 180, "uvlock": false }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "fence/nether_slats_ew", "uvlock": false }
        }
    ]
}
